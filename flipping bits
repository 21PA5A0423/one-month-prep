import java.util.Scanner;

public class Solution {


public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    int n = in.nextInt();
    long max = (long) Integer.MAX_VALUE * 2 + 1;
    for(int i = 0; i < n; i++) {
        System.out.println(~in.nextLong() & max);
    }
}
}
